{
    "name": "postsimple/filament-postsimple",
    "description": "Send your Filament resources to PostSimple for AI-powered social media content generation",
    "keywords": [
        "filament",
        "filament-plugin",
        "postsimple",
        "social-media",
        "ai",
        "laravel"
    ],
    "homepage": "https://github.com/postsimple/filament-postsimple",
    "license": "MIT",
    "authors": [
        {
            "name": "PostSimple",
            "email": "contact@postsimple.nl",
            "homepage": "https://postsimple.app"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0|^4.0|^5.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0|^13.0"
    },
    "autoload": {
        "psr-4": {
            "PostSimple\\FilamentPostSimple\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PostSimple\\FilamentPostSimple\\FilamentPostSimpleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

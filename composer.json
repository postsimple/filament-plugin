{
    "name": "postsimple/filament-postsimple",
    "description": "Send your Filament resources to PostSimple for AI-powered social media content generation",
    "keywords": [
        "filament",
        "filament-plugin",
        "postsimple",
        "social-media",
        "ai",
        "laravel"
    ],
    "homepage": "https://github.com/postsimple/filament-postsimple",
    "license": "MIT",
    "authors": [
        {
            "name": "PostSimple",
            "email": "contact@postsimple.nl",
            "homepage": "https://postsimple.app"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^3.0|^4.0|^5.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.14.0",
        "spatie/laravel-settings": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PostSimple\\FilamentPostSimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PostSimple\\FilamentPostSimple\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PostSimple\\FilamentPostSimple\\FilamentPostSimpleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
